{% set transaction = field.value %}
<table class="table table-bordered">
    <tr><th>Payment Mode</th><td>{{ transaction.paymentMode }}</td></tr>
    <tr><th>Payment Id</th><td>{{ transaction.razorpayPaymentId }}</td></tr>
    <tr><th>Payment Date</th><td>{{ transaction.transactionDate|date('m/d/Y g:i A') }}</td></tr>
    {% if transaction.attachment %}
        <tr>
            <th>Attachment</th>
            <td>
                <a href="{{ transaction.attachmentWebPath }}" target="_blank"><img src="{{ transaction.attachmentPath | imagine_filter('xs') }}" alt></a>
                {#<a href="#" class="ea-lightbox-thumbnail" data-ea-lightbox-content-selector="#ea-lightbox-{{ field.uniqueId }}">
                    <img src="{{ transaction.attachmentPath | imagine_filter('xs') }}" class="img-fluid">
                </a>
                <div id="ea-lightbox-{{ field.uniqueId }}" class="ea-lightbox">
                    <img src="{{ transaction.attachmentWebPath }}">
                </div>#}
            </td>
        </tr>
    {% endif %}
</table>